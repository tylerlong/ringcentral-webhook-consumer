{
  "name": "ringcentral-webhook-consumer",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "description": "Out-of-box consumer for RingCentral WebHook, mainly for testing and troubleshooting purpose.",
  "main": "build/electron.bundle.js",
  "scripts": {
    "upgrade": "yarn-upgrade-all && yarn remove webpack && yarn add --dev webpack@3.11.0 && yarn remove babel-preset-ts-node6-bluebird && yarn add --dev babel-preset-ts-node6-bluebird@1.0.1",
    "build": "webpack --progress --colors",
    "build:watch": "yarn build --watch",
    "release": "yarn build -p",
    "start": "electron .",
    "dist": "yarn release && rm -rf dist && electron-builder build --mac --win --linux --publish always",
    "dist:dev": "rm -rf dist && electron-builder build --mac --win --linux",
    "download": "node -r babel-register -r babel-polyfill build/downloader.js"
  },
  "repository": "git@github.com:tylerlong/ringcentral-webhook-consumer.git",
  "author": {
    "name": "Tyler Liu",
    "email": "tyler.liu@ringcentral.com"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "detect-port": "^1.2.2",
    "electron-log": "^2.2.14",
    "electron-updater": "^2.21.4",
    "express": "^4.16.3",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-ts-node6-bluebird": "1.0.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "standard": "^11.0.1",
    "style-loader": "^0.20.3",
    "webpack": "3.11.0",
    "yarn-upgrade-all": "^0.3.0"
  },
  "build": {
    "appId": "ringcentral-webhook-consumer.ringcentral.com",
    "productName": "RingCentral WebHook Consumer",
    "files": [
      "build/index.html",
      "build/electron.bundle.js",
      "build/preload.bundle.js",
      "build/index.bundle.js",
      "build/index.bundle.css",
      "!node_modules${/*}"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "x": 128,
          "y": 160
        },
        {
          "x": 384,
          "y": 160,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "compression": "maximum"
  }
}
